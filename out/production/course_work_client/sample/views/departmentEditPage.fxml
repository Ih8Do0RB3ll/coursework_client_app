                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Департамент" />
                <TextField fx:id="depNameField" GridPane.columnIndex="1" />
            </children>
        </GridPane>
        <ButtonBar layoutX="41.0" layoutY="360.0" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="360.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#handleOk" text="OK" />

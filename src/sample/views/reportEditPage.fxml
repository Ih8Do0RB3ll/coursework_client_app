<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" styleClass="background" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.ReportEditPageController">
    <children>
        <GridPane styleClass="background" AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="140.0" minWidth="10.0" prefWidth="79.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="211.0" minWidth="10.0" prefWidth="211.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Username" />
                <Label text="Creation Date" GridPane.rowIndex="1" />
                <Label text="Department" GridPane.rowIndex="2" />
                <Label text="Error" GridPane.rowIndex="4" />
                <Label text="Danger Level" GridPane.rowIndex="5" />
                <TextField fx:id="usernameField" GridPane.columnIndex="1" disable="true" />
                <TextField fx:id="creationDateField" GridPane.columnIndex="1" GridPane.rowIndex="1" disable="true" />
                <TextField fx:id="departmentField" GridPane.columnIndex="1" GridPane.rowIndex="2" disable="true" />
                <ChoiceBox fx:id="errorBox" prefHeight="26.0" prefWidth="214.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                <ChoiceBox fx:id="dangerLevelBox" prefHeight="26.0" prefWidth="246.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            </children>
        </GridPane>
        <ButtonBar layoutX="41.0" layoutY="360.0" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="360.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#handleOk" text="OK" />
                <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
            </buttons>
        </ButtonBar>
    </children>
</AnchorPane>